{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "^2.7",
    "composer/package-versions-deprecated": "1.11.99.1",
    "doctrine/annotations": "^1.11",
    "doctrine/doctrine-bundle": "^2.2",
    "doctrine/doctrine-migrations-bundle": "^3.1.1",
    "doctrine/orm": "^2.8",
    "erusev/parsedown": "^1.7",
    "friendsofsymfony/jsrouting-bundle": "^3.0",
    "knplabs/knp-time-bundle": "^1.14",
    "lcobucci/jwt": "^4.1",
    "league/flysystem-aws-s3-v3": "^2.0",
    "nelmio/cors-bundle": "^2.2",
    "phpdocumentor/reflection-docblock": "^5.2",
    "phpstan/phpdoc-parser": "^1.14",
    "pyrrah/gravatar-bundle": "^1.1",
    "rayatomsk/paddle-api": "^0.6.0",
    "roukmoute/hashids-bundle": "^3.0",
    "sentry/sentry-symfony": "^4.2",
    "symfony/asset": "5.4.*",
    "symfony/config": "5.4.*",
    "symfony/console": "5.4.*",
    "symfony/css-selector": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/expression-language": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/form": "5.4.*",
    "symfony/framework-bundle": "5.4.*",
    "symfony/http-client": "5.4.*",
    "symfony/mailer": "5.4.*",
    "symfony/mercure-bundle": "^0.3.2",
    "symfony/messenger": "5.4.*",
    "symfony/monolog-bundle": "^3.7",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/proxy-manager-bridge": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/security-bundle": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/translation": "5.4.*",
    "symfony/twig-bundle": "5.4.*",
    "symfony/validator": "5.4.*",
    "symfony/webpack-encore-bundle": "^1.11",
    "symfony/yaml": "5.4.*",
    "symfonycasts/reset-password-bundle": "^1.9",
    "symfonycasts/verify-email-bundle": "^1.12",
    "tgalopin/html-sanitizer-bundle": "^1.4",
    "twig/cssinliner-extra": "^3.3",
    "twig/extra-bundle": "^3.3",
    "twig/inky-extra": "^3.3",
    "twig/markdown-extra": "^3.3",
    "vdb/php-spider": "^0.6.2",
    "willdurand/js-translation-bundle": "^5.0"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^6.5",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "friendsofphp/php-cs-fixer": "^3.0",
    "friendsoftwig/twigcs": "^6.0",
    "phpmd/phpmd": "^2.11",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^1.4",
    "phpstan/phpstan-symfony": "^1.1",
    "symfony/browser-kit": "5.4.*",
    "symfony/maker-bundle": "^1.26",
    "symfony/phpunit-bridge": "^5.3",
    "symfony/stopwatch": "^5.3",
    "symfony/web-profiler-bundle": "^5.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
	"allow-plugins": {
		"composer/*": true,
		"phpstan/extension-installer": true,
		"symfony/*": true
	},
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.4.*",
      "docker": true
    },
	"runtime": {
		"class": "Symfony\\Component\\Runtime\\SymfonyRuntime"
	}
  }
}
